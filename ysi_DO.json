{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "width":200,
  "autosize": {
    "type": "fit",
    "resize": "true"
  },
  "autosize":{"type":"fit",
              "resize":"true"},
  "data": {"url":"data/ysi_burrpondtest.csv"},
  "mark": "line",
  "encoding": {
    "y": {"field": "Depth",
          "type": "quantitative",
          "sort": "descending",
          "axis": {"title": "Depth (m)"}},
    "x": {"field": "DO",
          "type": "quantitative",
          "axis": {"title": "DO (mg/L)"},
          "scale": {"zero":"true"}
    }
  },
  "config":{
    "view": {"stroke": "transparent"},
    "axis":{"titleFontSize": 15,"grid": false}
  }
}
